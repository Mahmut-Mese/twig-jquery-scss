
{{ widget('datalayer', 'product_detail', {layerData: {product: product, breadcrumbs: breadcrumb}} ) }}

<main role="main">

    <div class="product-detail-page-wrapper">
        <ul class="breadcrumbs with-go-back">
            <li class="go-back-link">
                <a href="/">
					 <i class="svg-home svg-home-dims"></i>
                    {# <i class="svg-slide-arrow-left svg-slide-arrow-left-dims"></i> #}
                </a>
            </li>
            {% for cat in breadcrumb %}
                <li>
                    <a class="text-capital" href="/{{ cat.url }}">{{ cat.name | translate }}</a>
                </li>
            {% endfor %}
            <li>
                <a class="text-capital" href="javascript:void(0);">{{ product.name | raw | capitalize }}</a>
            </li>
        </ul>
        <div class="title-box">
            <h1 class="text-capital">{{ product.manufacturer | raw | capitalize }}
                <span class="description text-capital">{{ product.name | raw | capitalize }}</span>
            </h1>
        </div>


        <div class="container">
            <div class="product-detail-slider pos-relative">

				<div class="badges">
					{% for badge in badges.desktop_image %}
						{% if loop.index <= 2 %}
							<img src="{{ badge }}" />
						{% endif %}
					{% endfor %}
				</div>


                {% if product.media_gallery %}
                    <div class="holder">
                        {% for key, image in product.media_gallery %}
                            <a href="{{ image.url|resize('1000x1000') }}" class="slide" data-fancybox="gallery">
                                <img src="{{ image.url|resize('750x750') }}" alt="{{ product.manufacturer }} {{ product.name }}">
                            </a>
                        {% endfor %}
                    </div>
					{% if product.media_gallery | length > 1 %}
						<a href="javascript:" class="arrow next"></a>
						<a href="javascript:" class="arrow prev"></a>
					{% endif %}

                    <span class="add-to-favourites-button add-wishlist" data-favourited="false"
                          data-product-id="{{ product.id }}" data-sku="{{ product.sku }}">
                <i class="svg-favourite svg-favourite-dims"></i>
                </span>
                    <div class="prices">
                        {% if product.special_price %}
                            <span>{{ product.price|price }}</span>
						{% endif %}

						<strong class="{% if product.rule_info is empty %} text-highlight {% endif %}">{{ product|price }}</strong>
					</div>

                    {% if product.rule_info %}
                        {% set firstItem = product.rule_info[0] %}
                        {% if firstItem.label or product.third_price %}
                            <div class="discount ">
                                {{ firstItem.label }}
                                {% if product.third_price %}
                                    <span class="text-warning">{{ product.third_price|price }}</span>
                                {% endif %}
                            </div>
                        {% endif %}
                    {% endif %}
                    <div class="div-padding-bold"></div>
                {% endif %}
            </div>

            {% if product.color_options %}
            <div class="choices-box">

                    <div class="color-selection">
                        {% for key,color_option in product.color_options %}
                            <div class="form-check">
                                <input class="form-check-input" type="radio" name="colorSelection"
                                       id="colorSelection{{ key }}"
                                       value="blue" {% if color_option.sku == product.sku %} checked {% endif %}>
                                <label class="form-check-label" for="colorSelection{{ key }}">
                                    <a href="{{ color_option.url }}">
                                        <img width="58px" height="58px" src="{{ color_option.image|resize('58x58') }}">
                                    </a>
                                </label>
                            </div>
                        {% endfor %}
                    </div>

            </div>
            {% endif %}
            <div class="choices-box pb-1">
                <div class="size-selection">
                    <div class="title">
                        <span>Beden Seçiniz</span>
                        <a href="javascript:" data-toggle="modal" data-target="#sizeTableModal">Beden Tablosu</a>
                    </div>
                    <div class="size-list">
                        {% for key, option in product.options %}

                            <div class="form-check {% if option.is_in_stock is empty %}disabled{% endif %}">
                                <input class="form-check-input input-radio{% if product.options|length == 1 %} selected{% endif %}"
                                       type="radio" name="sizeSelection"
                                       id="sizeSelection{{ key }}"
                                       value="{{ option.option_value }}"
                                        {% if product.options|length == 1 %} checked{% endif %}
                                        {% if product.options|length == 1 %} data-selected="1" {% endif %}
                                        {% if option.is_in_stock is empty %} data-soldout="1" {% endif %}
                                       data-name="option_sku_{{ product.id }}" data-value="{{ option.sku }}"
                                       data-option-id="{{ option.entity_id }}"
                                       data-option="{{ option.option_value }}">
                                <label class="form-check-label"
                                       for="sizeSelection{{ key }}">{{ option.option_value }}</label>
                            </div>

                        {% endfor %}

                    </div>
                </div>
            </div>
            <div class="basket-button">
                    <a id="add2cart" data-has-option="{% if product.options %}1{% else %}0{% endif %}"
                       data-product-id="{{ product.id }}"
                       data-sku="{{ product.options|length == 1 ? product.options|first.sku : product.sku }}"
                       class="add-to-cart linkto"
                       href="">{% if product.qty %}{{ 'SEPETE EKLE' | translate }}{% else %}{{ 'HABER VER' | translate }}{% endif %}</a>
            </div>
            <button id="clickSepeteekle" type="button" class="add-to-cart-button hidden">SEPETE EKLE <i
                        class="fa fa-angle-right"></i></button>
            {#            <button type="button" class="get-informed-instock-button">HABER VER <i class="fa fa-angle-right"></i></button>#}
            <div class="bg-white p-2">
                <div class="shipping-info">
                    {# {{ product_cargo['firstDate'] | date("d.m.Y") }} - {{ product_cargo['lastDate'] | date("d.m.Y") }}
                    tarihleri arasında kargoda. #}

					En geç {{ date_format_tr(product_cargo['lastDate'], 'd F l') }} günü kargoda
                </div>
