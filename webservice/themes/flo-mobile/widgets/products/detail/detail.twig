			"item": {
				"@id": "{{ config('env.webservice.url.web') }}{{ item.url }}", "name ": "{{ item.name }}"
			}
		},
		{% endfor %}
		{
			"@type": "ListItem",
			"position": {{ breadcrumb | length + 1 }},
			"item": {
				"@id": "{{ config('env.webservice.url.web') }}", "name ": "{{ product.name }}"
			}
		}
	]
}
</script>
