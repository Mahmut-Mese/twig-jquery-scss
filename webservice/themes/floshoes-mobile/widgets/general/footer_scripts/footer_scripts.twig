{% else %}
    {% set themeName = config('env.webservice.theme.mobile')|join(':') %}
    <script>
        var jqueryLoadCallback = function () {
            getScript('/pub/assets/dist/{{ themeName }}/{{ themeName }}-prod.js?v={{ git_version() }}', function () {
                applyCallbacks(domReadyCallbacks);
                getScript('/page/translations');
            });
        };
    </script>
    <script src="/pub/assets/lib/jquery/jquery.min.js" onload="jqueryLoadCallback()"></script>
{% endif %}

