<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for category in categories %}
{% if category.request_path starts with 'kampanya' %}
{% else %}<url>
            <loc>{{ config('env.webservice.url.web') }}{{ category.request_path }}</loc>
        </url>
        {% endif %}
    {% endfor %}
</urlset>
